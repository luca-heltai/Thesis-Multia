issn = {00457825},
journal = {Computer Methods in Applied Mechanics and Engineering},
keywords = {Collocation methods,Isogeometric analysis,Locking-free methods,NURBS,Spatial Timoshenko rod},
pages = {113--126},
publisher = {Elsevier B.V.},
title = {{Locking-free isogeometric collocation methods for spatial Timoshenko rods}},
url = {http://dx.doi.org/10.1016/j.cma.2013.03.009 http://ac.els-cdn.com/S0045782513000601/1-s2.0-S0045782513000601-main.pdf?\_tid=d69a654c-b35c-11e4-88a4-00000aacb361\&acdnat=1423817384\_900beec348f171412c42f5a2a115624c},
